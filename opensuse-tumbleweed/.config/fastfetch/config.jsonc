{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "raw",
        "source": "/home/che/.config/fastfetch/images/chameleon-kitty.bin",
        "position": "left",
        "color": {
            "1": "green"
        },
        "padding": {
            "top": 15,
            "left": 2,
            "right": 0
        }
    },
    "display": {
        "separator": " "
    },
    "modules": [
        "break",
        {
            "type": "custom",
            "format": "\u001b[33m┌──────────────────────Hardware──────────────────────┐"
        },
        {
            "type": "cpu",
            "key": "│ ┌{icon} ",
            "keyColor": "yellow"
        },
        {
            "type": "gpu",
            "key": "│ ├{icon} ",
            "keyColor": "yellow"
        },
        {
            "type": "memory",
            "key": "│ ├{icon} ",
            "keyColor": "yellow",
            "format": "{total} ({percentage})"
        },
        {
            "type": "disk",
            "key": "│ ├{icon} ",
            "keyColor": "yellow",
            "format": "{mountpoint} ({filesystem}) {size-total} ({size-percentage})"
        },
        {
            "type": "swap",
            "key": "│ └{icon} ",
            "keyColor": "yellow",
            "format": "swap {total} ({percentage})"
        },
        {
            "type": "custom",
            "format": "\u001b[33m└────────────────────────────────────────────────────┘"
        },
        "break",
        {
            "type": "custom",
            "format": "\u001b[32m┌──────────────────────Software──────────────────────┐"
        },
        {
            "type": "os",
            "key": "│ ┌{icon} ",
            "keyColor": "green"
        },
        {
            "type": "kernel",
            "key": "│ ├ ",
            "keyColor": "green"
        },
        {
            "type": "bios",
            "key": "│ ├{icon} ",
            "keyColor": "green"
        },
        {
            "type": "packages",
            "key": "│ ├{icon} ",
            "keyColor": "green"
        },
        {
            "type": "shell",
            "key": "└ └{icon} ",
            "keyColor": "green"
        },
        "break",
        {
            "type": "de",
            "key": "┌ ┌󰧨 ",
            "keyColor": "green"
        },
        {
            "type": "lm",
            "key": "│ ├{icon} ",
            "keyColor": "green"
        },
        {
            "type": "wm",
            "key": "│ ├{icon} ",
            "keyColor": "green"
        },
        {
            "type": "wmtheme",
            "key": "│ ├{icon} ",
            "keyColor": "green"
        },
        {
            "type": "terminal",
            "key": "│ └ ",
            "keyColor": "green"
        },
        {
            "type": "custom",
            "format": "\u001b[32m└────────────────────────────────────────────────────┘"
        },
        "break",
        {
            "type": "custom",
            "format": "\u001b[35m┌──────────────────────Temporal──────────────────────┐"
        },
        {
            "type": "command",
            "key": "│ OS Age   ",
            "keyColor": "magenta",
            "text": "birth_install=$(stat -c %W /); current=$(date +%s); time_progression=$((current - birth_install)); days_difference=$((time_progression / 86400)); echo  $days_difference days"
        },
        {
            "type": "uptime",
            "key": "│ Uptime   ",
            "keyColor": "magenta"
        },
        {
            "type": "datetime",
            "key": "│ DateTime ",
            "keyColor": "magenta"
        },
        {
            "type": "custom",
            "format": "\u001b[35m└────────────────────────────────────────────────────┘"
        },
        {
            "type": "custom",
            "format": "                     \u001b[31m⬤ \u001b[32m⬤ \u001b[33m⬤ \u001b[34m⬤ \u001b[35m⬤ \u001b[36m⬤ \u001b[0m"
        },

        "break"
    ]
}
